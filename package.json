{
  "name": "@folio/idm-connect",
  "version": "1.0.0",
  "description": "Search IDM",
  "main": "src/index.js",
  "repository": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.20.1"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "yarn run test:jest",
    "test:jest": "jest --ci --coverage --silent",
    "lint": "eslint src",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-idm-connect ./translations/ui-idm-connect/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.10.4",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.4.0 || ^2.4.0",
    "@folio/stripes-core": "^8.0.0",
    "@formatjs/cli": "^4.2.29",
    "@jest/globals": "^26.6.2",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "inflected": "^2.0.4",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-junit": "^12.0.0",
    "mocha": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-idm-connect.meta.title",
    "route": "/idm-connect",
    "home": "/idm-connect",
    "queryResource": "query",
    "okapiInterfaces": {
      "idm-connect": "1.0",
      "users": "15.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.idm-connect.enabled",
        "displayName": "UI: ui-idm-connect module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.idm-connect.enabled",
        "displayName": "Settings (idm-connect): display list of settings pages",
        "subPermissions": [
          "module.idm-connect.enabled",
          "settings.enabled"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.view",
        "displayName": "IDM connect: Can view idm connect",
        "description": "Can view idm connect",
        "subPermissions": [
          "module.idm-connect.enabled",
          "settings.idm-connect.enabled",
          "idmconnect.contract.get",
          "idmconnect.searchidm.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.create-edit",
        "displayName": "IDM connect: Can create and edit idm connect",
        "description": "Can create and edit idm connect",
        "subPermissions": [
          "ui-idm-connect.view",
          "idmconnect.contract.post",
          "idmconnect.contract.put"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.delete",
        "displayName": "IDM connect: Can delete idm connect",
        "description": "Can delete single draft contract in idm connect",
        "subPermissions": [
          "ui-idm-connect.view",
          "idmconnect.contract.delete"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "IDM connect",
        "title": "idmConnect"
      }
    ]
  }
}
