{
  "name": "@folio/idm-connect",
  "version": "1.0.0",
  "description": "Search IDM",
  "main": "src/index.js",
  "repository": "folio-org/ui-idm-connect",
  "license": "Apache-2.0",
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "yarn run test:jest",
    "test:jest": "jest --ci --coverage",
    "lint": "eslint src",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-idm-connect ./translations/ui-idm-connect/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.10.4",
    "@folio/eslint-config-stripes": "^7.0.0",
    "@folio/jest-config-stripes": "^1.0.0",
    "@folio/stripes": "^9.0.0",
    "@folio/stripes-cli": "^3.0.0",
    "@folio/stripes-components": "^12.0.0",
    "@folio/stripes-core": "^10.0.0",
    "@folio/stripes-smart-components": "^9.0.0",
    "@formatjs/cli": "^4.2.29",
    "eslint": "^7.32.0",
    "identity-obj-proxy": "^3.0.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.8.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prop-types": "^15.6.0",
    "react-final-form": "^6.5.9",
    "react-router-prop-types": "^1.0.5",
    "redux-form": "^8.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^9.0.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^5.8.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "stripes": {
    "actsAs": [
      "app",
      "settings"
    ],
    "displayName": "ui-idm-connect.meta.title",
    "route": "/idm-connect",
    "home": "/idm-connect?filters=status.updated&sort=lastName",
    "queryResource": "query",
    "okapiInterfaces": {
      "idm-connect": "1.0",
      "users": "15.0 16.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.idm-connect.enabled",
        "displayName": "UI: ui-idm-connect module is enabled",
        "visible": false
      },
      {
        "permissionName": "settings.idm-connect.enabled",
        "displayName": "Settings (idm-connect): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-idm-connect.all",
        "displayName": "IDM connect: All permissions",
        "subPermissions": [
          "module.idm-connect.enabled",
          "settings.idm-connect.enabled",
          "ui-idm-connect.view",
          "ui-idm-connect.searchidm",
          "ui-idm-connect.create",
          "ui-idm-connect.edit-delete",
          "ui-idm-connect.changeubreadernumber"
        ],
        "visible": false
      },
      {
        "permissionName": "ui-idm-connect.view",
        "displayName": "IDM connect: View Walk-in contracts",
        "description": "Can view Walk-in contracts",
        "subPermissions": [
          "module.idm-connect.enabled",
          "idmconnect.contract.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.searchidm",
        "displayName": "IDM connect: Search IDM",
        "description": "Can search IDM",
        "subPermissions": [
          "idmconnect.searchidm.get",
          "users.collection.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.create",
        "displayName": "IDM connect: Create Walk-in contracts",
        "description": "Can create Walk-in connect",
        "subPermissions": [
          "idmconnect.contract.post",
          "idmconnect.contract.transmit.get",
          "idmconnect.searchidm.get"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.edit-delete",
        "displayName": "IDM connect: Edit and delete Walk-in contracts",
        "description": "Can edit and delete Walk-in contracts",
        "subPermissions": [
          "idmconnect.contract.put",
          "idmconnect.contract.transmit.get",
          "idmconnect.contract.delete"
        ],
        "visible": true
      },
      {
        "permissionName": "ui-idm-connect.changeubreadernumber",
        "displayName": "IDM connect: Change card number",
        "description": "Can change card number",
        "subPermissions": [
          "idmconnect.ubreadernumber.post",
          "idmconnect.ubreadernumber.delete",
          "idmconnect.searchidm.get",
          "users.collection.get"
        ],
        "visible": true
      }
    ],
    "icons": [
      {
        "name": "app",
        "alt": "IDM connect",
        "title": "idmConnect"
      }
    ]
  }
}
